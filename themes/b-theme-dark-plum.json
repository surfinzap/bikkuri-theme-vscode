  "tokenColors": [
    {
      "settings": {
        "foreground": "#c8c5cf"
      }
    },
    {
      "scope": "emphasis",
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "scope": "strong",
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "scope": "header",
      "settings": {
        "foreground": "#6b91b3"
      }
    },
    {
      "scope": [
        "comment",
        "punctuation.definition.comment",
        "string.comment.buffered.block.pug",
        "string.quoted.docstring.multi.python"
      ],
      "settings": {
        "foreground": "#564f64",
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        "constant",
        "support.constant",
        "variable.arguments"
      ],
      "settings": {
        "foreground": "#a9a0e6"
      }
    },
    {
      "scope": "constant.rgb-value",
      "settings": {
        "foreground": "#c8c5cf"
      }
    },
    {
      "scope": "entity.name.selector",
      "settings": {
        "foreground": "#61b6b4"
      }
    },
    {
      "scope": "entity.other.attribute-name",
      "settings": {
        "foreground": "#81add5"
      }
    },
    {
      "scope": [
        "entity.name.tag",
        "punctuation.tag"
      ],
      "settings": {
        "foreground": "#9cb062"
      }
