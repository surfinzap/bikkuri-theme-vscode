{
  "mode-type": "dark",
  "mode-name": "Dark",
  "shadow": "{{ color.black }}",

  "test-repl": "{{ red.60 }}",

  "surface" : {
    "sunken": "{{ tint.90 }}",
    "base": "{{ tint.100 }}",
    "raised-low": "{{ tint.70 }}{{ opacity.med-low }}",
    "raised-med": "{{ tint.70 }}{{ opacity.med }}",
    "raised-high": "{{ tint.70 }}{{ opacity.med-high }}",
    "none": "{{ tint.100 }}{{ opacity.none }}"
  },
  "text": {
    "base": "{{ tint.30 }}",
    "mid": "{{ tint.40 }}",
    "dim": "{{ tint.50 }}",
    "disabled": "{{ tint.60 }}",
    "on-color": "{{ tint.100 }}"
  },
  "icon": {
    "base": "{{ tint.40 }}",
    "mid": "{{ tint.50 }}"
  },
  "border": {
    "base": "{{ tint.50 }}",
    "mid": "{{ tint.70 }}",
    "dim": "{{ tint.80 }}",
    "none": "{{ surface.base }}{{ opacity.none }}"
  },
  "ui": {
    "accent1": {
      "base": "{{ blue.40 }}",
      "dim": "{{ blue.50 }}"
    },
    "accent2": {
      "base": "{{ violet.40 }}",
      "dim": "{{ violet.50 }}"
    },
    "accent3": {
      "base": "{{ teal.40 }}",
      "dim": "{{ teal.50 }}"
    },
    "success": {
      "base": "{{ green.40 }}",
      "dim": "{{ green.50 }}"
    },
    "danger": {
      "base": "{{ red.50 }}",
      "dim": "{{ red.60 }}"
    },
    "warning": {
      "base": "{{ yellow.30 }}",
      "dim": "{{ yellow.40 }}"
    },
    "info": {
      "base": "{{ blue.40 }}",
      "dim": "{{ blue.50 }}"
    }
  },

  "syntax": {
    "01": {
      "base": "{{ red.50 }}",
      "dim": "{{ red.60 }}"
    },
    "02": {
      "base": "{{ orange.40 }}",
      "dim": "{{ orange.50 }}"
    },
    "03": {
      "base": "{{ yellow.30 }}",
      "dim": "{{ yellow.40 }}"
    },
    "04": {
      "base": "{{ green.40 }}",
      "dim": "{{ green.50 }}"
    },
    "05": {
      "base": "{{ teal.40 }}",
      "dim": "{{ teal.50 }}"
    },
    "06": {
      "base": "{{ blue.40 }}",
      "dim": "{{ blue.50 }}"
    },
    "07": {
      "base": "{{ violet.40 }}",
      "dim": "{{ violet.50 }}"
    }
  }
}