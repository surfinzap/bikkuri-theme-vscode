  },
  "tokenColors": [
    {
      "settings": {
        "background": "{{red1}}",
        "foreground": "{{gray80}}"
      }
    },
    {
      "scope": "emphasis",
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "scope": "strong",
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "scope": "header",
      "settings": {
        "foreground": "{{blue1}}"
      }
    },
    {
      "scope": [
        "comment",
        "punctuation.definition.comment",
        "string.comment.buffered.block.pug",
      ],
      "settings": {
        "foreground": "{{gray40}}",
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        "constant",
        "support.constant",
        "variable.arguments"
      ],
      "settings": {
        "foreground": "{{violet2}}"
      }
    },
    {
      "scope": "constant.rgb-value",
      "settings": {
        "foreground": "{{gray80}}"
      }
    },
    {
      "scope": "entity.name.selector",
      "settings": {
        "foreground": "{{teal2}}"
      }
    },
    {
      "scope": "entity.other.attribute-name",
      "settings": {
        "foreground": "{{blue2}}"
      }
    },
    {
      "scope": [
        "entity.name.tag",
        "punctuation.tag"
      ],
      "settings": {
        "foreground": "{{green2}}"
      }
    },
    {
      "scope": [
        "invalid",
        "invalid.illegal"
      ],
      "settings": {
        "foreground": "{{red1}}"
      }
    },
    {
      "scope": "invalid.deprecated",
      "settings": {
        "foreground": "{{violet1}}"
      }
    },
    {
      "scope": "meta.selector",
      "settings": {
        "foreground": "{{teal2}}"
      }
    },
    {
      "scope": "meta.preprocessor",
      "settings": {
        "foreground": "{{orange2}}"
      }
    },
    {
      "scope": "meta.preprocessor.string",
      "settings": {
        "foreground": "{{green2}}"
      }
    },
    {
      "scope": "meta.preprocessor.numeric",
      "settings": {
        "foreground": "{{green2}}"
      }
    },
    {
